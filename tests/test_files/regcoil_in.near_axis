! For documentation of these input parameters and their default values, see the manual.
! Stellarator asymmetric plasma boundary
! stellarator symmetric winding surface
! no symmetry imposed on current potential
&regcoil_nml
  save_level=0
  nlambda = 3
  lambda_min = 1e-15
  lambda_max = 1e+100

  ntheta_plasma = 32
  ntheta_coil   = 32
  nzeta_plasma  = 32
  nzeta_coil    = 32
  mpol_potential = 8
  ntor_potential = 8

  geometry_option_plasma = 2
  wout_filename = 'wout_sym.nc'

  geometry_option_coil = 3
  nescin_filename = 'nescin_sym.out' 
  ! separation = 0.03

  symmetry_option = 1
/
